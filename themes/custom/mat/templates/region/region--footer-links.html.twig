{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
  ]
%}
{% if content %}
  <div{{ attributes.addClass(classes) }}>
    <a href="https://www.donatelife.net" target="_blank" class="logo_donate-life">
      <img src="/themes/custom/mat/assets/images/donate-life-logo.svg" alt="Donate Life">
      <img src="/themes/custom/mat/assets/images/donate-life-text.svg" alt="Donate Life">
    </a>

    <a href="/" class="logo"><img src="/themes/custom/mat/assets/images/logo-inspired.svg" alt="Mid-America Transplant"></a>

    {{ content }}
  </div>
{% endif %}